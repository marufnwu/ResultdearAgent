<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.skithub.resultdear.agent">

    <application
        android:name=".application.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ResultdearAgent"
        android:usesCleartextTraffic="true">
        <activity
            android:theme="@style/Theme.ResultdearAgent.NoActionBar"
            android:name=".view.activities.UserListActivity"
            android:exported="true" />
        <activity
            android:name=".view.activities.SignupActivity"
            android:exported="true"
            android:label="Sign Up"
            android:theme="@style/Theme.ResultdearAgent.NoActionBar" />

        <activity
            android:name=".view.activities.EntryActivity"
            android:exported="true"
            android:theme="@style/Theme.ResultdearAgent.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activities.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.ResultdearAgent.NoActionBar" />
    </application>

</manifest>